{
  "name": "coc-format-json",
  "version": "0.2.1",
  "description": "format JSON with the power of coc.nvim",
  "author": "Gerald <gera2ld@live.com>",
  "license": "MIT",
  "scripts": {
    "dev": "rollup -wc rollup.conf.js",
    "clean": "del-cli dist types",
    "ci": "npm run lint",
    "lint": "eslint --ext .ts .",
    "build:types": "tsc",
    "build:js": "rollup -c rollup.conf.js",
    "build": "run-s ci clean build:types build:js",
    "prepare": "husky install"
  },
  "main": "dist/index.common.js",
  "files": [
    "dist",
    "types"
  ],
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "typings": "types/index.d.ts",
  "devDependencies": {
    "@gera2ld/plaid": "~2.5.1",
    "@gera2ld/plaid-common-ts": "~2.5.1",
    "@gera2ld/plaid-rollup": "~2.5.0",
    "coc.nvim": "^0.0.80",
    "del-cli": "^4.0.1",
    "husky": "^7.0.4",
    "vscode-languageserver-types": "^3.16.0"
  },
  "dependencies": {
    "@babel/runtime": "^7.17.7",
    "@gera2ld/format-json": "^0.2.4",
    "json5": "^2.2.0"
  },
  "engines": {
    "coc": ">= 0.0.70"
  },
  "keywords": [
    "coc.nvim",
    "json",
    "javascript"
  ],
  "repository": "git@github.com:gera2ld/coc-format-json.git"
}
