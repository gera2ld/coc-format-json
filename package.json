{
  "name": "coc-format-json",
  "version": "0.2.0",
  "description": "format JSON with the power of coc.nvim",
  "author": "Gerald <i@gerald.top>",
  "license": "MIT",
  "husky": {
    "hooks": {
      "pre-push": "npm run lint"
    }
  },
  "scripts": {
    "dev": "rollup -wc rollup.conf.js",
    "clean": "del dist types",
    "build:js": "rollup -c rollup.conf.js",
    "prebuild": "npm run ci && npm run clean",
    "prepublishOnly": "npm run build",
    "ci": "npm run lint",
    "lint": "eslint --ext .ts .",
    "build": "tsc && npm run build:js"
  },
  "main": "dist/index.common.js",
  "files": [
    "dist",
    "types"
  ],
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "typings": "types/index.d.ts",
  "devDependencies": {
    "@gera2ld/plaid": "~2.0.0",
    "@gera2ld/plaid-common-ts": "^2.0.5",
    "@gera2ld/plaid-rollup": "~2.0.0",
    "coc.nvim": "^0.0.77",
    "del-cli": "^3.0.1",
    "husky": "^4.2.5"
  },
  "dependencies": {
    "@babel/runtime": "^7.10.5",
    "@gera2ld/format-json": "^0.1.0"
  },
  "engines": {
    "coc": ">= 0.0.70"
  },
  "keywords": [
    "coc.nvim",
    "json",
    "javascript"
  ],
  "repository": "git@github.com:gera2ld/coc-format-json.git"
}
