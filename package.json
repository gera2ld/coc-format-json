{
  "name": "coc-format-json",
  "version": "0.2.0",
  "description": "format JSON with the power of coc.nvim",
  "author": "Gerald <i@gerald.top>",
  "license": "MIT",
  "scripts": {
    "dev": "rollup -wc rollup.conf.js",
    "clean": "del-cli dist types",
    "ci": "npm run lint",
    "lint": "eslint --ext .ts .",
    "build:types": "tsc",
    "build:js": "rollup -c rollup.conf.js",
    "build": "run-s ci clean build:types build:js",
    "postinstall": "husky install",
    "pinst": "pinst",
    "prepublishOnly": "run-s build 'pinst -- --disable'",
    "postpublish": "pinst --enable"
  },
  "main": "dist/index.common.js",
  "files": [
    "dist",
    "types"
  ],
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "typings": "types/index.d.ts",
  "devDependencies": {
    "@gera2ld/plaid": "~2.2.2",
    "@gera2ld/plaid-common-ts": "~2.2.0",
    "@gera2ld/plaid-rollup": "~2.2.0",
    "coc.nvim": "^0.0.80",
    "del-cli": "^3.0.1",
    "husky": "^5.1.1",
    "pinst": "^2.1.6",
    "vscode-languageserver-types": "^3.16.0"
  },
  "dependencies": {
    "@babel/runtime": "^7.13.7",
    "@gera2ld/format-json": "^0.2.0",
    "json5": "^2.2.0"
  },
  "engines": {
    "coc": ">= 0.0.70"
  },
  "keywords": [
    "coc.nvim",
    "json",
    "javascript"
  ],
  "repository": "git@github.com:gera2ld/coc-format-json.git"
}
